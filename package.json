    "webpack": "^1.13.1"
  },
  "scripts": {
    "compile": "NODE_ENV=production babel-node ./node_modules/.bin/webpack --optimize-minimize --progress --config ./webpack/prod.config",
    "clean:dist": "rm -rf static/dist",
    "deploy": "npm run clean:dist && npm run compile && npm run prod",
    "prod": "DEBUG=app:* NODE_PATH=src NODE_ENV=production babel-node src/server/index.js",
    "start": "concurrently --kill-others --raw \"npm run start:client:server\" \"npm run start:server\"",
    "start:server": "DEBUG=app:* NODE_PATH=src babel-node -- src/server/index.js",
    "start:client:server": "DEBUG=app:* babel-node webpack/dev.server.js"
  }
}
